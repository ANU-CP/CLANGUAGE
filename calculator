#include<stdio.h>
void main()
{
    int a,b;
    printf("enter two numbers");
    scanf("%d%d",&a,&b);
    char op;
    printf("enter symbol");
    scanf(" %c",&op);
    switch(op)
    {
        case '+': printf("the sum is %d",a+b);
                  break;
        case '-': printf("the sub is%d",a-b);
                  break;
        case '*': printf("the mul is%d",a*b);
                  break;
        case '/': printf("the div is%d",a/b);
                   break;
        case '%': printf("the md  is%d",a%b);
                  break;
        default: printf("wrong input");
    }
}
